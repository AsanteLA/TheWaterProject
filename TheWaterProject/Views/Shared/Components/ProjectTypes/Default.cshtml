

@model IQueryable<string>

<div class="d-grid gap-2">
    <a class="btn btn-outline-secondary" asp-controller="Home" asp-action="Index" asp-route-projectType="">Home</a>
    @foreach (string pt in Model ?? Enumerable.Empty<string>()) //pt = project type
    {
        <a class="btn btn-outline-secondary" asp-controller="Home" asp-action="Index" asp-route-projectType="@pt" asp-route-pageNum="1">@pt</a>
    }
</div>